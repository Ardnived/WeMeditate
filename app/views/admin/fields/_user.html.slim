
= f.input :email, as: :email
= f.input :role, collection: assignable_roles.map {|k, v| [human_enum_name(User, :role, k), k]}
= f.input :languages, collection: I18n.available_locales.collect {|l| [I18nData.languages[l.to_s.upcase], l]}, prompt: t('misc.locale.all_languages'), input_html: { multiple: true }
